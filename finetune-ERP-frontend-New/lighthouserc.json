{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "url": ["/about/", "/contact/", "/locate/", "/terms-and-conditions/"]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.9 }],
        "categories:accessibility": ["error", { "minScore": 0.9 }],
        "categories:seo": ["error", { "minScore": 0.9 }],
        "largest-contentful-paint": ["error", { "maxNumericValue": 2500, "numericUnit": "millisecond" }]
      }
    }
  }
}
